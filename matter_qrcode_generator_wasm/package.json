{
    "name": "matter_qrcode_generator_wasm",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "@html-eslint/eslint-plugin": "^0.13.1",
        "@html-eslint/parser": "^0.13.1",
        "css-loader": "^6.7.1",
        "eslint": "^8.12.0",
        "eslint-plugin-html": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "http-server": "^14.1.0",
